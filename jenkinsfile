pipeline{
    agent any
    // to set path for maven 
   // environemnt {
   //   PATH = C:/Maven/Maven/Maven/apache-maven-3.8.4-bin/apache-maven-3.8.4/bin:$PATH 
   // }
    stages{
        stage('git checkout'){
            steps{
                git 'https://github.com/harikesh12/hello-world.git'
            }
        }
        stage('maven build'){
            steps{
               withMaven(maven: 'maven') {
                 sh "mvn clean package"
        }
         }
       }
    }
}
