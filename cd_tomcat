pipeline{
    agent any
    tools{
        jfrog 'jfrog-cli'
    }
    stages{
        stage('download jfrog latest artifact '){
            steps{
              sh "rm -rf jfrog_artifact_download"  
              sh "mkdir jfrog_artifact_download"
              sh "cd jfrog_artifact_download"
           //   sh "wget https://jfrogpoc1.jfrog.io/artifactory/axa-maven-snapshot/"
               jf 'rt dl axa-maven-snapshot/webapp/target/'
            }
        }
    }
}

